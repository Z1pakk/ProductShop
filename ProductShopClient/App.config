<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICalculatorContract" />
                <binding name="BasicHttpsBinding_ICalculatorContract">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_IAccountService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://wcfservicetest.gear.host/Calculator.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICalculatorContract"
                contract="ServiceReference1.ICalculatorContract" name="BasicHttpBinding_ICalculatorContract" />
            <endpoint address="https://wcfservicetest.gear.host/Calculator.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_ICalculatorContract"
                contract="ServiceReference1.ICalculatorContract" name="BasicHttpsBinding_ICalculatorContract" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/Bankomat"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAccountService"
                contract="Proxy.IAccountService" name="BasicHttpBinding_IAccountService" />
        </client>
    </system.serviceModel>
</configuration>